	month = jan,
	year = {2016},
	keywords = {amyloid fibril, amyloidosis, cytotoxic mechanism, extracellular chaperone, nucleation-dependent polymerization model, pathological molecular environment, thioflavin T},
	pages = {n/a--n/a}
}

