	pages = {441--465},
	file = {ScienceDirect Full Text PDF:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/CMA3F8JS/Härd i Lendel - 2012 - Inhibition of Amyloid Formation.pdf:application/pdf;ScienceDirect Snapshot:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/TZDVHE2E/S002228361200006X.html:text/html}
}

