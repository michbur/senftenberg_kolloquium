	pages = {e54175},
	file = {Full Text PDF:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/XGAHUKUM/Tsolis i in. - 2013 - A Consensus Method for the Prediction of ‘Aggregat.pdf:application/pdf;Snapshot:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/9FT8AKAB/article.html:text/html}
