	month = aug,
	year = {2016},
	file = {Full Text PDF:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/HG93MGEN/Burdukiewicz i in. - 2016 - Prediction of amyloidogenicity based on the n-gram.pdf:application/pdf;Snapshot:/home/michal/.zotero/zotero/pwethkyi.default/zotero/storage/W6WB52ZS/2390.html:text/html}
